{
  "version": 2,
  "builds": [
    {
      "src": "api/salvar_pedido.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "src/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
